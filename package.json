{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Authentication with MERN (MongoDB, EXPRESS, React/Redux, NodeJS)",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm && npm build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Kakuheiki",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.6.5",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "combobox": "^1.0.1",
    "concurrently": "^5.3.0",
    "downshift": "^6.0.6",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.0.0-beta.2",
    "mongoose": "^5.10.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regex": "^0.1.1",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "redux-devtools": "^3.7.0"
  }
}
